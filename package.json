{
  "name": "automation-analytics",
  "version": "1.1.0",
  "private": false,
  "dependencies": {
    "@ansible/ansible-ui-framework": "^2.4.1114",
    "@ansible/react-json-chart-builder": "^2.0.1",
    "@babel/runtime": "^7.22.10",
    "@patternfly/patternfly": "^5.2.0",
    "@patternfly/react-charts": "^7.0.0",
    "@patternfly/react-core": "^5.2.0",
    "@patternfly/react-table": "^5.2.0",
    "@patternfly/react-tokens": "^5.2.0",
    "@redhat-cloud-services/frontend-components": "^4.2.4",
    "@redhat-cloud-services/frontend-components-notifications": "^4.1.0",
    "@redhat-cloud-services/frontend-components-utilities": "^4.0.6",
    "@sinclair/typebox": "^0.31.1",
    "@unleash/proxy-client-react": "^1.0.4",
    "classnames": "^2.5.1",
    "d3": "^5.16.0",
    "eslint-plugin-cypress": "^2.14.0",
    "moment": "^2.29.4",
    "monaco-editor": "^0.45.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "monaco-yaml": "5.1.1",
    "plotly.js": "^2.25.2",
    "query-string": "^7.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.3.1",
    "react-plotly.js": "^2.6.0",
    "react-redux": "^7.2.9",
    "react-router-dom": "^6.15.0",
    "redux": "^4.2.1",
    "redux-promise-middleware": "^6.1.3",
    "streamsaver": "^2.0.6",
    "styled-components": "^5.3.11",
    "uuid": "^8.3.2",
    "victory-core": "^36.8.5",
    "web-streams-polyfill": "^3.2.1"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*{.js,.ts,.tsx}",
      "!src/__tests__/**/*",
      "!src/**/types.ts"
    ],
    "setupFiles": [
      "<rootDir>/config/setupTests.js"
    ],
    "roots": [
      "<rootDir>/src/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/__tests__/"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "transformIgnorePatterns": [
      "@ansible/ansible-ui-framework"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@cfaester/enzyme-adapter-react-18": "^0.7.1",
    "@redhat-cloud-services/eslint-config-redhat-cloud-services": "^1.3.0",
    "@redhat-cloud-services/frontend-components-config": "^4.7.3",
    "@redhat-cloud-services/frontend-components-config-utilities": "^1.5.31",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/react-plotly.js": "^2.6.0",
    "@types/react-router-dom": "^5.3.3",
    "@types/streamsaver": "^2.0.1",
    "@types/styled-components": "^5.1.26",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^27.5.1",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "css-loader": "^6.8.1",
    "cypress": "^12.17.4",
    "cypress-wait-until": "^1.7.2",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint": "^8.9.0",
    "fetch-mock-jest": "^1.5.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "node-fetch": "^2.6.12",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.8.1",
    "redux-mock-store": "^1.5.4",
    "stylelint": "^13.13.1",
    "stylelint-config-recommended-scss": "^4.3.0",
    "stylelint-scss": "^3.21.0",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "victory-cursor-container": "^36.6.12",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^4.10.0"
  },
  "scripts": {
    "test": "TZ=UTC jest --verbose",
    "test:watch": "jest --coverage=false --watch",
    "lint": "(eslint config src) && (stylelint 'src/**/*.scss' --config .stylelintrc.json)",
    "lint:fix": "eslint config src --fix",
    "start": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js",
    "start:container": "NODE_ENV=development webpack serve --config config/dev.webpack.config.js --host 0.0.0.0",
    "build:prod": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "build": "NODE_ENV=production webpack --config config/prod.webpack.config.js",
    "deploy": "npm-run-all build:prod lint test",
    "deploy-only": "npm-run-all build:prod",
    "verify": "npm-run-all build:prod lint test",
    "cy:open": "DEBUG=cypress cypress open",
    "cy:headless": "DEBUG=cypress cypress run --browser=chrome",
    "cy:lint": "eslint config cypress",
    "cy:lint-fix": "eslint config cypress --fix"
  },
  "insights": {
    "appname": "automation-analytics"
  },
  "overrides": {
    "tough-cookie": "^4.1.3"
  }
}
